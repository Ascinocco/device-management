# Docker Compose environment variables
# Copy to .env and fill in real values

# Clerk (auth provider)
CLERK_SECRET_KEY=sk_test_...
CLERK_JWT_ISSUER=https://clerk.example.com
VITE_CLERK_PUBLISHABLE_KEY=pk_test_...

# Internal service tokens (shared secret for service-to-service auth)
TENANCY_SERVICE_TOKEN=dev-internal-token
DEVICE_SERVICE_TOKEN=dev-shared-secret

# Email (Resend)
RESEND_API_KEY=re_...
RESEND_FROM="Devices <noreply@example.com>"
